     xmlns:dct="http://purl.org/dc/terms/"
	 xmlns:pdo="http://www.stkos-ontology/domain/plantbiodiversity#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:tea="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/">
        <rdfs:label>Tea Ontology</rdfs:label>
        <dct:modified>04/17/2016</dct:modified>
        <skos:changeNote>Added flavors, adjusted individuals</skos:changeNote>
        <owl:versionIRI>
		http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/v1

		</owl:versionIRI>
        <dct:creator xml:lang="en">Cara Reedy</dct:creator>
        <dct:creator xml:lang="en">Katie Chastain</dct:creator>
        <dct:publisher xml:lang="en">Tea Team</dct:publisher>
        <dct:abstract xml:lang="en">The goal of this ontology is to deliver a reasonable list of recommended tea blends, given a list of teas a user has or might purchase, based on characteristics of those listed teas.</dct:abstract>
        <owl:priorVersion rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/"/>
        <owl:imports rdf:resource="http://www.nanopub.org/nschema#"/>
        <owl:imports rdf:resource="http://www.stkos-ontology/domain/plantbiodiversity#"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:Ontology>
    


