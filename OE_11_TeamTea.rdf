    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/brews -->

    <owl:ObjectProperty rdf:about="&tea;brews">
        <rdfs:label>brews</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string" xml:lang="en">identifies the steeped drinks a particular brewing temperature range is appropriate for</skos:definition>
        <rdfs:domain rdf:resource="&tea;BrewingTemperature"/>
        <rdfs:range rdf:resource="&tea;SteepedDrink"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/brewsAt -->

    <owl:ObjectProperty rdf:about="&tea;brewsAt">
        <rdfs:label>brews at</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string" xml:lang="en">relates a part of a particular plant to its appropriate brewing temperature(s)</skos:definition>
        <rdfs:domain rdf:resource="&tea;PlantPart"/>
        <rdfs:range rdf:resource="&tea;BrewingTemperature"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/contains -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/contains">
        <rdfs:label>contains</rdfs:label>
        <skos:definition>used for blend to indicate which steepedDrinks are part of that blend</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/flavorOf -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/flavorOf">
        <rdfs:label>flavor of</rdfs:label>
        <skos:definition>the steepedDrinks a particular flavor flavors</skos:definition>
        <owl:inverseOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/hasFlavor"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/hasBrewingTemperature -->

    <owl:ObjectProperty rdf:about="&tea;hasBrewingTemperature">
        <rdfs:label>has brewing temperature</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string" xml:lang="en">specifies the brewing temperature range for a particular steeped drink</skos:definition>
        <rdfs:subPropertyOf>
            <owl:propertyChainAxiom rdf:parseType="Collection">
                <rdf:Description rdf:about="&tea;madeFromPart"/>
                <rdf:Description rdf:about="&tea;brewsAt"/>
            </owl:propertyChainAxiom>
        </rdfs:subPropertyOf>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/hasCaffeine -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/hasCaffeine">
        <rdfs:label>has caffeine</rdfs:label>
        <skos:definition>has caffeination value of caffeine or no caffeine</skos:definition>
        <rdfs:range rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/Caffeination"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/hasFlavor -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/hasFlavor">
        <rdfs:label>has flavor</rdfs:label>
        <skos:definition>the flavor of a particular steepedDrink</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/isCompatibleWith -->

    <owl:ObjectProperty rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/isCompatibleWith">
        <rdf:type rdf:resource="&owl;ReflexiveProperty"/>
        <rdfs:label xml:lang="en">is compatible with</rdfs:label>
        <skos:definition xml:lang="en">relation between two flavors that are considered to have some compatibility with each other</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/madeFromPart -->

    <owl:ObjectProperty rdf:about="&tea;madeFromPart">
        <rdfs:label>made from part</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string" xml:lang="en">specifies the part of a given plant from which a steeped drink is made</skos:definition>
        <rdfs:domain rdf:resource="&tea;SteepedDrink"/>
        <rdfs:range rdf:resource="&tea;PlantPart"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/madeFromPlant -->

    <owl:ObjectProperty rdf:about="&tea;madeFromPlant">
        <rdfs:label xml:lang="en">made from plant</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string" xml:lang="en">specifies the plant from which a steeped drink is made</skos:definition>
        <rdfs:domain rdf:resource="&tea;SteepedDrink"/>
        <rdfs:range rdf:resource="&tea;Plant"/>
    </owl:ObjectProperty>
    


