    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/ASTASpiceFlavor -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/ASTASpiceFlavor">
        <rdfs:label xml:lang="en">ASTA spice flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/Flavor"/>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
        <skos:definition>a flavor which comes from a spice defined by the ASTA spice list</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/AgedRipePu-ErhTea -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/AgedRipePu-ErhTea">
        <rdfs:label xml:lang="en">aged raw pu-erh tea</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/Pu-ErhTea"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/hasBrewingTemperature"/>
                <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/HighBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source>http://www.seriouseats.com/2015/08/what-is-puerh-tea-where-to-buy.html</dct:source>
        <skos:definition>A pu-erh tea that is at least three years old</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/AllspiceFlavor -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/AllspiceFlavor">
        <rdfs:label xml:lang="en">allspice flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/SpicyFlavor"/>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of allspice, the dried berries of Pimenta dioica</skos:definition>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/AnimalFlavor -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/AnimalFlavor">
        <rdfs:label xml:lang="en">animal flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/Flavor"/>
        <dct:source>Adapted from Tea Cupping Standards (http://www.worldoftea.org/tea-cupping-standards/), Stone Leaf Tea&apos;s Tea Guru (http://www.stoneleaftea.com/tea-guru/), and The Australian Tea Maters&apos; Tea Flavour Wheel (https://australianteamasters.wordpress.com/2014/04/30/the-tea-flavour-wheel/)</dct:source>
        <skos:definition xml:lang="en">a flavor that is musky or evocative of leather</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/AniseFlavor -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/AniseFlavor">
        <rdfs:label xml:lang="en">anise flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/SpicyFlavor"/>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of anise, Pimpinella ansium</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/ArilPart -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/ArilPart">
        <rdfs:label xml:lang="en">aril plant part</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/PlantPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/brewsAt"/>
                <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/LowBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/brewsAt"/>
                <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/MediumBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/brewsAt"/>
                <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/HighBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>the aril of a plant, a growth that partially or fully covers a seed and may look like a fruit itself</skos:definition>
        <dct:source>Adapted from Merriam-Webster online dictionary</dct:source>
    </owl:Class>
	
	
    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/Aspalathus -->
	<owl:Class rdf:about="&tea;Aspalathus">
        <rdfs:label xml:lang="en">aspalathus</rdfs:label>
        <rdfs:subClassOf rdf:resource="&dwc;Genus"/>
        <rdfs:subClassOf>
            <owl:Restriction>
               <owl:onProperty rdf:resource="&pdo;has_species"/>
               <owl:allValuesFrom rdf:resource="&tea;Aspalathus_linearis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pdo;belong_to_family"/>
                <owl:hasValue rdf:resource="&pdo;Fabaceae"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string" xml:lang="en">a genus of flowering plants in the legume family, Fabaceae, that has species including aspalathus linearis</skos:definition>
        <dct:source rdf:resource="https://en.wikipedia.org/wiki/Aspalathus"/>
    </owl:Class>

    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/Aspalathus_linearis -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/Aspalathus_linearis">
        <rdfs:label xml:lang="en">Aspalathus linearis</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/Plant"/>
        <skos:definition xml:lang="en">a plant from the Fabaceae family; the leaves are used to make rooibos tea</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BanchaGreenTea -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BanchaGreenTea">
        <rdfs:label xml:lang="en">bancha green tea</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/GreenTea"/>
        <skos:definition>A green tea from Japan produced from unshaded steamed leaves that are picked in summer or autumn</skos:definition>
        <dct:source>http://infohost.nmt.edu/~armiller/japanese/tea.htm</dct:source>
        <skos:altLabel>番茶</skos:altLabel>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BarkPart -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BarkPart">
        <rdfs:label xml:lang="en">bark part</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/PlantPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/brewsAt"/>
                <owl:someValuesFrom rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/HighBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source>Adapted from Merriam-Webster online dictionary</dct:source>
        <skos:definition>the bark of a plant, the tough external covering of the stems or roots of woody plants</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BasilFlavor -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BasilFlavor">
        <rdfs:label xml:lang="en">basil flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/ASTASpiceFlavor"/>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of basil, the leaves of Ocimum basilicum</skos:definition>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BayLeafFlavor -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BayLeafFlavor">
        <rdfs:label xml:lang="en">bay leaf flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/HerbalFlavor"/>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of bay leaves, a spice made from the leaves of one of several trees in the Lauraceae or Myrtaceae families</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BerryPart -->

    <owl:Class rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/BerryPart">
        <rdfs:label xml:lang="en">berry part</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_11_TeaTeam/FruitPart"/>
        <skos:definition>culinarily, a fruit that is small and does not have a large seed within it, usually juicy</skos:definition>
        <dct:source>Adapted from Merriam-Webster online dictionary</dct:source>
    </owl:Class>
    


