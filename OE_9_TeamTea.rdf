<!DOCTYPE rdf:RDF [
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY np "http://www.nanopub.org/nschema#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdfg-1 "http://www.w3.org/2004/03/trix/rdfg-1/" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY tea "http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/" >
]>


<rdf:RDF xmlns="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/"
     xml:base="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/"
     xmlns:rdfg-1="http://www.w3.org/2004/03/trix/rdfg-1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:np="http://www.nanopub.org/nschema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:tea="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/">
        <rdfs:label>Tea Ontology</rdfs:label>
        <dct:creator xml:lang="en">Cara Reedy</dct:creator>
        <dct:creator xml:lang="en">Katie Chastain</dct:creator>
        <owl:imports rdf:resource="http://www.nanopub.org/nschema#"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:Ontology>
    


