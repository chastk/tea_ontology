     -->

    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/brews -->

    <owl:ObjectProperty rdf:about="&tea;brews">
        <skos:definition>the steepedDrinks a particular brewing temperature range can steep</skos:definition>
        <rdfs:domain rdf:resource="&tea;BrewingTemperature"/>
        <owl:inverseOf rdf:resource="&tea;hasBrewingTemperature"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/brewsAt -->

    <owl:ObjectProperty rdf:about="&tea;brewsAt">
        <skos:definition xml:lang="en">Relates a Plant Part to its appropriate Brewing Temperature(s)</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/contains -->

    <owl:ObjectProperty rdf:about="&tea;contains">
        <skos:definition>used for blend to indicate which steepedDrinks are part of that blend</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/flavorOf -->

    <owl:ObjectProperty rdf:about="&tea;flavorOf">
        <skos:definition>the steepedDrinks a particular flavor flavors</skos:definition>
        <owl:inverseOf rdf:resource="&tea;hasFlavor"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/fromSource -->

    <owl:ObjectProperty rdf:about="&tea;fromSource"/>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/hasBrewingTemperature -->

    <owl:ObjectProperty rdf:about="&tea;hasBrewingTemperature">
        <skos:definition>the brewing temperature range used for a particular steepedDrink</skos:definition>
        <rdfs:range rdf:resource="&tea;BrewingTemperature"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&tea;madeFromPart"/>
            <rdf:Description rdf:about="&tea;brewsAt"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/hasCaffeine -->

    <owl:ObjectProperty rdf:about="&tea;hasCaffeine">
        <skos:definition>has caffeination value of caffeine or no caffeine</skos:definition>
        <rdfs:range rdf:resource="&tea;Caffeination"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/hasFlavor -->

    <owl:ObjectProperty rdf:about="&tea;hasFlavor">
        <skos:definition>the flavor of a particular steepedDrink</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/madeFromPart -->

    <owl:ObjectProperty rdf:about="&tea;madeFromPart">
        <skos:definition>defines what kind of plant part a tisane is made from</skos:definition>
        <rdfs:range rdf:resource="&tea;PlantPart"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/withStrength -->

    <owl:ObjectProperty rdf:about="&tea;withStrength">
        <rdfs:domain rdf:resource="&tea;FlavorCompatibility"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/ASTASpiceFlavor -->

    <owl:Class rdf:about="&tea;ASTASpiceFlavor">
        <rdfs:label xml:lang="en">ASTA spice flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;Flavor"/>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
        <skos:definition>a flavor which comes from a spice defined by the ASTA spice list</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/AgedRipePu-ErhTea -->

    <owl:Class rdf:about="&tea;AgedRipePu-ErhTea">
        <rdfs:label xml:lang="en">aged raw pu-erh tea</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;Pu-ErhTea"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&tea;hasBrewingTemperature"/>
                <owl:someValuesFrom rdf:resource="&tea;HighBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source>http://www.seriouseats.com/2015/08/what-is-puerh-tea-where-to-buy.html</dct:source>
        <skos:definition>A pu-erh tea that is at least three years old</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/AllspiceFlavor -->

    <owl:Class rdf:about="&tea;AllspiceFlavor">
        <rdfs:label xml:lang="en">allspice flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="&tea;SpicyFlavor"/>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of allspice, the dried berries of Pimenta dioica</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/AnimalFlavor -->

    <owl:Class rdf:about="&tea;AnimalFlavor">
        <rdfs:label xml:lang="en">animal flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;Flavor"/>
        <dct:source>Adapted from Tea Cupping Standards (http://www.worldoftea.org/tea-cupping-standards/), Stone Leaf Tea&apos;s Tea Guru (http://www.stoneleaftea.com/tea-guru/), and The Australian Tea Maters&apos; Tea Flavour Wheel (https://australianteamasters.wordpress.com/2014/04/30/the-tea-flavour-wheel/)</dct:source>
        <skos:definition xml:lang="en">a flavor that is musky or evocative of leather</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/AniseFlavor -->

    <owl:Class rdf:about="&tea;AniseFlavor">
        <rdfs:label xml:lang="en">anise flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="&tea;SpicyFlavor"/>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of anise, Pimpinella ansium</skos:definition>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/ArilPart -->

    <owl:Class rdf:about="&tea;ArilPart">
        <rdfs:label xml:lang="en">aril plant part</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;PlantPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&tea;brewsAt"/>
                <owl:someValuesFrom rdf:resource="&tea;MediumBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&tea;brewsAt"/>
                <owl:someValuesFrom rdf:resource="&tea;LowBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&tea;brewsAt"/>
                <owl:someValuesFrom rdf:resource="&tea;HighBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition>the aril of a plant, a growth that partially or fully covers a seed and may look like a fruit itself</skos:definition>
        <dct:source>Adapted from Merriam-Webster online dictionary</dct:source>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BanchaGreenTea -->

    <owl:Class rdf:about="&tea;BanchaGreenTea">
        <rdfs:label xml:lang="en">bancha green tea</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;GreenTea"/>
        <skos:definition>A green tea from Japan produced from unshaded steamed leaves that are picked in summer or autumn</skos:definition>
        <dct:source>http://infohost.nmt.edu/~armiller/japanese/tea.htm</dct:source>
        <skos:altLabel>番茶</skos:altLabel>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BarkPart -->

    <owl:Class rdf:about="&tea;BarkPart">
        <rdfs:label xml:lang="en">bark part</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;PlantPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&tea;brewsAt"/>
                <owl:someValuesFrom rdf:resource="&tea;HighBrewingTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source>Adapted from Merriam-Webster online dictionary</dct:source>
        <skos:definition>the bark of a plant, the tough external covering of the stems or roots of woody plants</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BasilFlavor -->

    <owl:Class rdf:about="&tea;BasilFlavor">
        <rdfs:label xml:lang="en">basil flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;ASTASpiceFlavor"/>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of basil, the leaves of Ocimum basilicum</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BayLeafFlavor -->

    <owl:Class rdf:about="&tea;BayLeafFlavor">
        <rdfs:label xml:lang="en">bay leaf flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="&tea;HerbalFlavor"/>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of bay leaves, a spice made from the leaves of one of several trees in the Lauraceae or Myrtaceae families</skos:definition>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BerryPart -->

    <owl:Class rdf:about="&tea;BerryPart">
        <rdfs:label xml:lang="en">berry part</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;FruitPart"/>
        <skos:definition>culinarily, a fruit that is small and does not have a large seed within it, usually juicy</skos:definition>
        <dct:source>Adapted from Merriam-Webster online dictionary</dct:source>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BitterFlavor -->

    <owl:Class rdf:about="&tea;BitterFlavor">
        <rdfs:label xml:lang="en">bitter flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;Flavor"/>
        <skos:definition xml:lang="en">a flavor that is acrid</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BlackCarawayFlavor -->

    <owl:Class rdf:about="&tea;BlackCarawayFlavor">
        <rdfs:label xml:lang="en">black caraway flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="&tea;SpicyFlavor"/>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
        <skos:definition xml:lang="en">a flavor listed in the ASTA list that is reminiscent of black caraway, also known as black cumin, the seeds of the planf Nigella sativa</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BlackPepperFlavor -->

    <owl:Class rdf:about="&tea;BlackPepperFlavor">
        <rdfs:label xml:lang="en">black pepper flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="&tea;SpicyFlavor"/>
        <dct:source>ASTA spice list (http://www.astaspice.org/download/9423/)</dct:source>
        <skos:definition>a flavor listed in the ASTA list that is reminiscent of black pepper, the dried fruits of the flowering vine Piper nigrum</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BlackTea -->

    <owl:Class rdf:about="&tea;BlackTea">
        <rdfs:label xml:lang="en">black tea</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;Tea"/>
        <skos:altLabel>紅茶</skos:altLabel>
        <dct:source>Tea: The Drink that Changed the World</dct:source>
        <skos:definition>Tea that has been withered, rolled, oxidized, and dried; completely oxidized</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/Blend -->

    <owl:Class rdf:about="&tea;Blend">
        <rdfs:label xml:lang="en">blend</rdfs:label>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&tea;contains"/>
                <owl:onClass rdf:resource="&tea;SteepedDrink"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&tea;DomainConcept"/>
        <dct:source>Merriam-Webster online dictionary</dct:source>
        <skos:definition>the combination of at least 2 steepedDrinks</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BrewingTemperature -->

    <owl:Class rdf:about="&tea;BrewingTemperature">
        <rdfs:label xml:lang="en">brewing temperature</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&tea;HighBrewingTemperature"/>
                    <rdf:Description rdf:about="&tea;LowBrewingTemperature"/>
                    <rdf:Description rdf:about="&tea;MediumBrewingTemperature"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&tea;ValuePartition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&tea;brews"/>
                <owl:onClass rdf:resource="&tea;SteepedDrink"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source>http://www.silkroadteas.com/how-to-brew-loose-leaf-tea-brewing-instructions/</dct:source>
        <skos:definition>The temperature of the water in which a steeped beverage is steeped</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/BudPart -->

    <owl:Class rdf:about="&tea;BudPart">
        <rdfs:label xml:lang="en">bud part</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;FlowerPart"/>
        <skos:definition>an underdeveloped flower</skos:definition>
        <dct:source>Adapted from Merriam-Webster online dictionary</dct:source>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/Caffeinated -->

    <owl:Class rdf:about="&tea;Caffeinated">
        <rdfs:subClassOf rdf:resource="&tea;Caffeination"/>
        <owl:disjointWith rdf:resource="&tea;Decaffeinated"/>
        <skos:definition>Containing caffeine</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/CaffeinatedSteepedDrink -->

    <owl:Class rdf:about="&tea;CaffeinatedSteepedDrink">
        <rdfs:label xml:lang="en">caffeinated steeped drink</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&tea;SteepedDrink"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&tea;hasCaffeine"/>
                        <owl:someValuesFrom rdf:resource="&tea;Caffeinated"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&tea;SteepedDrink"/>
        <owl:disjointWith rdf:resource="&tea;DecaffeinatedSteepedDrink"/>
        <skos:definition>a steeped drink that contains caffeine</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/Caffeination -->

    <owl:Class rdf:about="&tea;Caffeination">
        <rdfs:label xml:lang="en">caffeination</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&tea;Caffeinated"/>
                    <rdf:Description rdf:about="&tea;Decaffeinated"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&tea;ValuePartition"/>
        <skos:definition>Whether a brew has caffeine in it or not</skos:definition>
    </owl:Class>
    


    <!-- http://tw.rpi.edu/web/Courses/Ontologies/2016/OE_9_TeaTeam/CapsicumsFlavor -->

    <owl:Class rdf:about="&tea;CapsicumsFlavor">
        <rdfs:label xml:lang="en">capsicums flavor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&tea;ASTASpiceFlavor"/>
        <rdfs:subClassOf rdf:resource="&tea;SpicyFlavor"/>
